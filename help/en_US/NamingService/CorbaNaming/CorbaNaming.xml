  <refnamediv>
    <refname>CorbaNaming</refname>

    <refpurpose>Reference of CorbaNaming Server </refpurpose>
  </refnamediv>

 
  
 
  <refsection>
    <title>Description</title>

    <para>This handle value is used as the reference of CorbaNaming service.</para>
  </refsection>

  <refsection>
    <title>Examples</title>

    <programlisting role="example">
      cn = CorbaNaming_create("localhost:2809");
      nameList = CorbaNaming_getRTCList(cn);
      rtobject=CorbaNaming_resolve(cn, "ysuga_ps.host_cxt/ConsoleIn.rtc");
      ...
      CorbaNaming_destroy(cn);</programlisting>
  </refsection>

