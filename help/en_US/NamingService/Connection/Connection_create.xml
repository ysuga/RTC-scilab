  <refnamediv>
    <refname>Connection_create</refname>

    <refpurpose>Create Reference of Connection </refpurpose>
  </refnamediv>

  <refsynopsisdiv>
    <title>Calling Sequence</title>

    <synopsis>cn=Connection_create(portA, portB)</synopsis>
  </refsynopsisdiv>
  <refsection>
    <title>Parameters</title>
    <variablelist>
      <varlistentry>
        <term>cn</term>
        <listitem>
          <para>Handle of Connection.</para>
        </listitem>
      </varlistentry>
    
      <varlistentry>
        <term>portA</term>
        <listitem>
          <para>Handle of Port</para>
        </listitem>
      </varlistentry>
      <varlistentry>
        <term>portB</term>
        <listitem>
          <para>Handle of Port</para>
        </listitem>
      </varlistentry>

    </variablelist>

  </refsection>

  
  <refsection>
    <title>Description</title>

    <para>This function create Connection</para>
  </refsection>

  <refsection>
    <title>Examples</title>
    <programlisting role="example">
      p1=RTObject_getPort(rto, "in");
      p2=RTObject_getPort(rto, "in");
      connection=Connection_create(p1, p2);</programlisting>
  </refsection>

