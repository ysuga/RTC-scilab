  <refnamediv>
    <refname>Port_getPortType</refname>

    <refpurpose>Get Port Type of Port of RTC</refpurpose>
  </refnamediv>


  <refsynopsisdiv>
    <title>Calling Sequence</title>

    <synopsis>ret=Port_getPortType(port)</synopsis>
  </refsynopsisdiv>
  <refsection>
    <title>Parameters</title>
    <variablelist>
      <varlistentry>
        <term>port</term>
        <listitem>
          <para>Handle of Port.</para>
        </listitem>
      </varlistentry>

      <varlistentry>
        <term>ret</term>
        <listitem>
          <para>string. Port Type.</para>
        </listitem>
      </varlistentry>

    </variablelist>

  </refsection>
  
  
  
  <refsection>
    <title>Description</title>

    <para>This function get Port profile (Port Type).</para>
  </refsection>

  <refsection>
    <title>Examples</title>

    <programlisting role="example">
      cn = CorbaNaming_create("localhost:2809");
      rtobject = CorbaNaming_resolve(cn, "ysuga_pc.host_cxt/ConsoleIn0.rtc");
      port1=RTObject_getPort(rtobject, "out");
      disp(Port_getPortType(port1)); </programlisting>
  </refsection>

